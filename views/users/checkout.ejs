  <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>  
  <div id="userData" data-user ='<%= JSON.stringify(user) %>'></div>
  <script>
    const applyCouponBtn = document.getElementById('apply-coupon-btn');
    const couponCodeInput = document.getElementById('coupon-code');
    const tickmark = document.getElementById('checktrue');
    const crossmark = document.getElementById('checkfalse');
    const discountVal = document.getElementById('discountVal');
    const totVal = document.getElementById('totVal');
    const Amount = document.getElementById('amount');
    const user = JSON.parse(document.getElementById('userData').dataset.user);

    // Function to update UI and user object when applying or removing coupon
    const updateCoupon = async (couponCode, remove = false) => {
        try {
            let response;
            if (remove) {
                response = await axios.post('/remove-coupon');
            } else {
                response = await axios.post('/check-coupon', { couponCode });
            }

            if (response.data.valid) {
                tickmark.style.display = remove? 'none' : 'flex';
                crossmark.style.display = 'none';
                discountVal.textContent = remove ? 0 : response.data.value;
                let tot = parseFloat(Amount.textContent) - parseFloat(discountVal.textContent);
                totVal.textContent = tot;
                applyCouponBtn.textContent = remove ? 'Apply Coupon' : 'Remove Coupon';
                couponCodeInput.disabled = remove ? false : true;
                couponCodeInput.placeholder = remove ? 'Coupon code' : 'Coupon Applied!';
                user.cart.discount = remove ? 0 : response.data.value;
            } else {
                crossmark.style.display = 'flex';
                tickmark.style.display = 'none';
                discountVal.textContent = 0;
                totVal.textContent = Amount.textContent - 0;
            }
        } catch (error) {
            console.error('Failed to apply coupon:', error);
        }
    };

    // Check if a coupon is already applied
    if (user.cart.discount !== 0) {
        tickmark.style.display = 'flex';
        applyCouponBtn.textContent = "Remove Coupon";
        couponCodeInput.disabled = true;
        couponCodeInput.placeholder = 'Coupon Applied!';
    }

    // Add event listener to apply or remove coupon
    applyCouponBtn.addEventListener('click', async () => {
        const couponCode = couponCodeInput.value;
        if (user.cart.discount !== 0) {
            // Coupon is applied, so remove it
            updateCoupon(couponCode, true);
        } else {
            // Coupon is not applied, so apply it
            updateCoupon(couponCode);
        }
    });
</script>
