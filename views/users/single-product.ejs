<script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
<script>
    const addToCart = document.getElementById('addToCart');
    addToCart.addEventListener('click', function(event) {
        event.preventDefault(); // Prevent default button behavior
        const productId = this.getAttribute('data-product-id');
        addToCartf(productId);
    });

    function addToCartf(productId) {
            console.log("add to cart function triggered :",productId )
            axios.post('/addToCart', {
                productId: productId
            })
            .then(function (response) {
                console.log(response.data);

                // Show the toast when product is added
                // const toast = document.getElementById('cartNotification');
                // toast.classList.add('show');
                if(response.status===201){
                  Toastify({
                  text: "Please Login",
                  duration: 800, 
                  gravity: "top", 
                  position: 'right',
                  backgroundColor: "red",
                  stopOnFocus: true, // Stop the timer when the toast is focused
                }).showToast();
                }else{
                  Toastify({
                    text: "Product Added to cart",
                    duration: 900, 
                    gravity: "top", 
                    position: 'right',
                    backgroundColor: "#203e5a",
                    stopOnFocus: true, // Stop the timer when the toast is focused
                  }).showToast();
                }

                // Hide the toast after 0.5 seconds
                setTimeout(function () {
                    toast.classList.remove('show');
                }, 2000);
            })
            .catch(function (error) {
                console.error('Error adding product to cart:', error);
                // Optionally, show an error message to the user
            });
        }
</script>

<!-- Add To Wishlist -->
<script>
  document.addEventListener('DOMContentLoaded', function() {
    const addToWishlistButtons = document.getElementsByClassName('addToWishlist');
  
    Array.from(addToWishlistButtons).forEach(function(button) {
      button.addEventListener('click', function(event) {
        event.preventDefault();
        const productId = this.getAttribute('data-product-id');
        addToWishlistf(productId);
      });
    });
  
    function addToWishlistf(productId) {
      console.log("add to wishlist function triggered :", productId)
      axios.post('/addToWishlist', {
          productId: productId
        })
        .then(function(response) {
          console.log(response.data);
          if(!response.data.userExist){
            Toastify({
            text: "Please login",
            duration: 700,
            gravity: "top",
            position: 'right',
            backgroundColor: "#f44336",
            stopOnFocus: true, // Stop the timer when the toast is focused
            }).showToast();
          }else if(!response.data.existInWishlist){
            Toastify({
            text: "Product removed from wishlist",
            duration: 700,
            gravity: "top",
            position: 'right',
            backgroundColor: "#f44336",
            stopOnFocus: true, // Stop the timer when the toast is focused
            }).showToast();
          }else{
            Toastify({
            text: "Product added to wishlist",
            duration: 900,
            gravity: "top",
            position: 'right',
            backgroundColor: "#203e5a",
            stopOnFocus: true, // Stop the timer when the toast is focused
            }).showToast();
          }
        })
        .catch(function(error) {
          console.error('Error adding product to wishlist:', error);
          // Optionally, show an error message to the user
        });
    }
  });
  
</script>
