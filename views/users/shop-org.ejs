    <script>
      // sorting

      document.getElementById('sortSelect').addEventListener('change', function() {
      filterProducts(); 
      });
      document.getElementById('searchProducts').addEventListener('submit',function(event){
        event.preventDefault();
        filterProducts();
      })

      // filtering
      // Retrieve the user object from the hidden input field
      // const userDataElement = document.getElementById('userData');
      // const userData = userDataElement ? JSON.parse(userDataElement.value) : null;
      const userDataElement = document.getElementById('userData');
      const userData = userDataElement && userDataElement.value ? JSON.parse(userDataElement.value) : null;

  
      // Add event listener to the filter form
      const filterForm = document.getElementById('filterForm');
      filterForm.addEventListener('submit', (event) => {
          event.preventDefault();
          filterProducts();
      });

    function filterProducts() {
      const selectedCategories = Array.from(document.querySelectorAll('input[name="categories"]:checked')).map(el => el.value);
      const selectedTags = Array.from(document.querySelectorAll('input[name="tags"]:checked')).map(el => el.value);
      const selectedSort = document.getElementById('sortSelect').value;
      const searchTerm = document.getElementById('searchInput').value;

      fetch('/filter', {
        method: 'POST',
        headers: {
          'Content-Type': 'application/json',
        },
        body: JSON.stringify({
          categories: selectedCategories,
          tags: selectedTags,
          sort: selectedSort,
          searchTerm: searchTerm, // Include the search term in the request body
        }),
      })
      .then(response => response.json())
      .then(data => {
        // Update the product listing based on the filtered and sorted data
        updateProductListing(data);
      })
      .catch(error => {
        console.error('Error:', error);
      });
    }

    function updateProductListing(products) {
      const productContainer = document.querySelector('.row .col-xl-9 .row');
      productContainer.innerHTML = '';

      if (products.length === 0) {
          // Show a message when no products are found
          productContainer.innerHTML = '<div class="col-xl-12 text-center mt-5"><h1 class="prod-msg">No products found</h1></div>';
      } else {
          products.forEach(product => {
              const productItem = `
                  <div class="col-xl-4 g-0">
                      <div class="product-item">
                          <div class="product-thumb" style="height:250px;width:250px;position: relative; overflow: hidden;">
                              <a href="/product/${product._id}" class="ms-3" style="max-height:250px;max-width:250px;" >
                                  <img class="h-100 w-100" style="object-fit:cover;position: absolute;"  src="${product.images[0]}" alt="Image-HasTech">
                              </a>
                          </div>
                          <div class="product-info">
                              <h4 class="title"><a href="/product/${product._id}">${product.productName}</a></h4>
                              <div class="prices">
                                  <span class="price">${product.price}</span>
                              </div>
                          </div>
                          <div class="product-action">
                              <button type="button" onclick="addToWishlistf('${product._id}', this)" id="addToWishlist-${product._id}" class="addToWishlist btn-product-wishlist bg-transparent" data-product-id="${product._id}">
                                  ${userData !== null && userData.wishlist.products.some(wProduct => wProduct._id.toString() === product._id.toString()) ? '<i class="bi bi-heart-fill text-danger fs-5"></i>' : '<i class="pe-7s-like"></i>'}
                              </button>
                              <div class="product-action-links">
                                <button type="button" onclick="addToCart('${product._id}')" class="addToCart btn-product-cart bg-transparent" data-product-id="${product._id}">
                                    <i class="bi bi-bag fs-5"></i>
                                </button>
                              </div>
                          </div>
                      </div>
                  </div>
              `;
              productContainer.insertAdjacentHTML('beforeend', productItem);
          });
      }
    }
    
  function addToWishlistf(productId, button) {
    axios.post('/addToWishlist', { productId })
      .then(function(response) {
        console.log(response.data);
        if(!response.data.userExist){
            Toastify({
            text: "Please login",
            duration: 700,
            gravity: "top",
            position: 'right',
            backgroundColor: "#f44336",
            stopOnFocus: true, // Stop the timer when the toast is focused
            }).showToast();
          }else if(response.data.existInWishlist){
            Toastify({
            text: "Product added to wishlist",
            duration: 900,
            gravity: "top",
            position: 'right',
            backgroundColor: "#203e5a",
            stopOnFocus: true, // Stop the timer when the toast is focused
            }).showToast();
          button.innerHTML = '<i class="bi bi-heart-fill text-danger fs-5"></i>'
          }else{
            Toastify({
            text: "product removed from wishlist",
            duration: 700,
            gravity: "top",
            position: 'right',
            backgroundColor: "#fc6603",
            stopOnFocus: true, // Stop the timer when the toast is focused
            }).showToast();
          button.innerHTML = '<i class="pe-7s-like"></i>';
          }
      })
      .catch(function(error) {
        console.error('Error adding product to wishlist:', error);
        // Optionally, show an error message to the user
      });
  }

  document.addEventListener('DOMContentLoaded', function() {
    const addToWishlistButtons = document.getElementsByClassName('addToWishlist');

    Array.from(addToWishlistButtons).forEach(function(button) {
      button.addEventListener('click', function(event) {
        event.preventDefault();
        const productId = this.getAttribute('data-product-id');
        addToWishlistf(productId, this); // Pass the productId and button
      });
    });
  });

    </script>
  
