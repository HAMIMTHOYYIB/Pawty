<script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
<!-- Add To Cart -->
<script>
document.addEventListener('DOMContentLoaded', function() {
  const addToCartButtons = document.getElementsByClassName('addToCart');

  Array.from(addToCartButtons).forEach(function(button) {
    button.addEventListener('click', function(event) {
      event.preventDefault();
      const productId = this.getAttribute('data-product-id');
      addToCartf(productId);
    });
  });
  function addToCartf(productId) {
  axios.post('/addToCart', {
    productId: productId
  })
  .then(function(response) {
    if (response.status === 201) {
      Toastify({
        text: response.data.error,
        duration: 900,
        gravity: "top",
        position: 'right',
        backgroundColor: "#f44336",
        stopOnFocus: true, // Stop the timer when the toast is focused
      }).showToast();
    } else {
      console.log(response.data);
      console.log("added to  cart")
      Toastify({
        text: "Product Added to cart",
        duration: 900, 
        gravity: "top", 
        position: 'right',
        backgroundColor: "#203e5a",
        stopOnFocus: true, // Stop the timer when the toast is focused
      }).showToast();
  
      setTimeout(function() {
        document.getElementById('cartNotification').classList.remove('show');
      }, 2000);
    }
  })
  .catch(function(error) {
    console.error('Error adding product to cart:', error);
    // Optionally, show an error message to the user
  });
}


});

</script>

