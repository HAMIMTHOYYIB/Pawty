                        <button type="button"  class="addToCart btn-shop-cart" data-product-id="<%= product._id %>">
                          Add To Cart
                        </button>
                      </td>
                      <% }else{ %>
                      <td class="product-stock-status">
                          <span class="wishlist-in-stock text-danger">Out Of Stock</span>
                      </td>
                      <td class="product-add-to-cart">
                        <button type="button" id="addToCart" class="addToCart btn-shop-cart" data-product-id="<%= product._id %>">
                          Add To Cart
                        </button>
