            <!-- Body: Body -->
            <div class="body d-flex py-3">  
                <div class="container-xxl"> 
                    <div class="row align-items-center"> 
                        <div class="border-0 mb-4"> 
                            <div class="card-header py-3 no-bg bg-transparent d-flex align-items-center px-0 justify-content-between border-bottom flex-wrap">
                                <h3 class="fw-bold mb-0">Order Details</h3>
                                <span class="text-muted fw-bold">#<%= order._id %></span>
                            </div>
                        </div>
                    </div> <!-- Row end  -->
                    <div class="row g-3 mb-3"> 
                        <div class="col-md-12 col-lg-12">
                            
                            <div class="col-lg-12 d-md-flex">
                                <div class="card col-lg-8 mb-2 me-2">
                                    <div class="d-flex pt-2 px-4">
                                        <label class="form-label text-primary fw-bold">Ordered On : </label>
                                        <p class="form-control-static fw-bold"><%= new Date(order.orderDate).toLocaleDateString('en-US', { day: 'numeric', month: 'long', year: 'numeric' }) %></p>
                                    </div>
                                    <div class="d-flex pb-2 px-4">
                                        <label class="form-label text-primary fw-bold">paymentMethod : </label>
                                        <p class="form-control-static fw-bold"><%= order.paymentMethod %></p>
                                    </div>
                                </div>
                                <div class="card col-lg-4 mb-2">
                                    <div class="d-flex px-4 pt-2">
                                        <label class="form-label text-primary fw-bold">Order Total : </label>
                                        <p class="form-control-static fw-bold"><%= order.total + order.discount %></p>
                                    </div>
                                    <div class="d-flex px-4 pb-2">
                                        <label class="form-label text-primary fw-bold">Discount : </label>
                                        <p class="form-control-static fw-bold"><%= order.discount %></p>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-12 d-md-flex">
                                <div class="card col-lg-8 me-2 mb-2">
                                    <h5 class="fw-bold m-3">Products</h5>
                                    <div class="card-body scrol"  style="height:280px; overflow-y: scroll;"> 
                                        <% updatedProducts.forEach(product => { %>
                                        <div class="d-md-flex col-lg-12 row" style="height:240px;border-right:5px solid #314d70;">
                                            <div class="col-lg-5">
                                                <img src="<%= product.images[0] %>" alt="Product Image" style="width:100%; height:100%;">
                                            </div>
                                            <div class="col-lg-7">
                                                <div class="row mt-3">
                                                    <div class=" d-flex">
                                                        <label class="form-label text-primary fw-bold">Name : </label>
                                                        <p class="form-control-static fw-bold"><%= product.productName %></p>
                                                    </div>
                                                    <div class=" d-flex">
                                                        <label class="form-label text-primary fw-bold">Price : </label>
                                                        <p class="form-control-static fw-bold"><%= product.price %></p>
                                                    </div>
                                                    <div class="d-flex">
                                                        <label class="form-label text-primary fw-bold"> Quantity : </label>
                                                        <p class="form-control-static fw-bold"><%= product.quantity %></p>
                                                    </div>
                                                    <div class="d-flex">
                                                        <label class="form-label text-primary fw-bold"> Status : </label>
                                                        <p class="form-control-static fw-bold"><%= product.status %></p>
                                                    </div>
                                                    <div class="d-flex">
                                                        <label class="form-label text-primary fw-bold"> Vendor : </label>
                                                        <p class="form-control-static fw-bold"><%= product.vendor %></p>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <% })%>
                                    </div>
                                </div>
                                <div class="card col-lg-4 mb-2">
                                    <h5 class="fw-bold m-2 d-flex justify-content-center">Delivery Address</h5>
                                    <div class="card-body"> 
                                        <div class="d-flex col-12 row">
                                            <!-- <div class="col-lg-6"> -->
                                                <div class="d-flex">
                                                    <label class="form-label text-primary fw-bold">Name : </label>
                                                    <p class="form-control-static fw-bold"><%= order.shippingAddress.name %></p>
                                                </div>
                                                <div class="d-flex">
                                                    <label class="form-label text-primary fw-bold">Phone : </label>
                                                    <p class="form-control-static fw-bold"><%= order.shippingAddress.phone %></p>
                                                </div>
                                                <div class="d-flex">
                                                    <label class="form-label text-primary fw-bold">Pincode : </label>
                                                    <p class="form-control-static fw-bold"><%= order.shippingAddress.pincode %></p>
                                                </div>
                                            <!-- </div>
                                            <div class="col-lg-6"> -->
                                                <!-- <div class="col-lg-12 d-flex"> -->
                                                <div class="d-flex">
                                                    <label class="form-label text-primary fw-bold">Locality : </label>
                                                    <p class="form-control-static fw-bold"><%= order.shippingAddress.locality %></p>
                                                </div>
                                                <div class="d-flex">
                                                    <label class="form-label text-primary fw-bold">Street : </label>
                                                    <p class="form-control-static fw-bold"><%= order.shippingAddress.street %></p>
                                                </div>
                                                <!-- </div>
                                                <div class="col-lg-12 d-flex"> -->
                                                <div class="d-flex">
                                                    <label class="form-label text-primary fw-bold">City : </label>
                                                    <p class="form-control-static fw-bold"><%= order.shippingAddress.city %></p>
                                                </div>
                                                <div class="d-flex">
                                                    <label class="form-label text-primary fw-bold">State : </label>
                                                    <p class="form-control-static fw-bold"><%= order.shippingAddress.state %></p>
                                                </div>
                                        </div>
                                    </div>          
                                </div>
                            </div>
                        </div>
                            
                        </div>
                    </div> <!-- Row end  -->
                </div>
            </div>           

        </div>                                 

    </div> 
    
