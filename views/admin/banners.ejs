                <script>
                    document.getElementById('offerBannerForm').addEventListener('submit', function(event) {
                        var fileInput = document.getElementById('offerBanner');
                        if (fileInput.files.length !== 3) {
                            event.preventDefault(); // Prevent form submission
                            document.getElementById('offerFileErrorMsg').classList.remove('d-none'); // Show error message
                        }
