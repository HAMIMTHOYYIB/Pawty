<!--wrapper start-->
<div class="wrapper">
    <!--== Start My Account Area Wrapper ==-->
    <section class="account-area">
      <div class="container">
        <div class="row">
          <div class="col-12">
            <div class="register-form-content">
              <b><span id="errMessage" class="text-danger"><%= passError %></span></b>
              <form action="/vendorSignup" method="post" onsubmit="return signupValid()">
                <div class="row">
                  <div class="col-12">
                    <div class="form-group">
                      <label for="name">Name<span class="required">*</span></label>
                      <input id="Name" name="name" class="form-control" type="text">
                      <span id="nameErr" class="text-danger"></span>
                    </div>
                  </div>
                  <div class="col-12">
                    <div class="form-group">
                      <label for="email">Email address <span class="required">*</span></label>
                      <input  id="email" name="email" class="form-control" type="text">
                      <span id="emailErr" class="text-danger"></span>
                    </div>
                  </div>
                  <div class="col-12">
                    <div class="form-group">
                      <label for="password">Password<span class="required">*</span></label>
                      <input id="password" class="form-control" name="password" type="password">
                      <label for="Confirm-password">Confirm-Password<span class="required">*</span></label>
                      <input id="confirm-password" class="form-control" name="confirmPassword" type="password">
                      <span id="passwordErr" class="text-danger"></span>
                    </div>
                  </div>
                  <div class="col-12">
                    <div class="form-group mb--0">
                      <button class="btn-register" onclick="signupValid" type="submit">Signup</button>
                      <!-- <a class="btn-register" href="shop.html">Signup</a> -->
                    </div>
                  </div>
                  <div class="col-12">
                    <div class="form-group account-info-group mb--0">
                      <a class="lost-password" href="/vendor">have an account?</a>
                    </div>
                  </div>
                </div>
              </form>
            </div>
          </div>
        </div>
      </div>
    </section>
    <!--== End My Account Area Wrapper ==-->
  <!--== End Aside Menu ==-->
</div>

<!--=======================Javascript============================-->
<script>
  function signupValid() {
    let Name = document.getElementById('Name').value;
    let email = document.getElementById('email').value;
    let passwrd = document.getElementById('password').value;
    let confirmPasswrd = document.getElementById('confirm-password').value;

    let errMessage = document.getElementById('errMessage');
    let passwordErr = document.getElementById('passwordErr');
    let nameErr = document.getElementById('nameErr');
    let emailErr = document.getElementById('emailErr');

    // Regex 
    let emailPattern = /^[^\s@]+@[^\s@]+\.[^\s@]+$/;
    let NamePattern = /^[a-zA-Z]{4,}$/;
    let passwordPattern = /^.{6,}$/;

    if (Name === '' || email === '' || passwrd === '' || confirmPasswrd === '') {
        errMessage.innerHTML = "Please fill the form";
        return false;
    } else {
        errMessage.innerHTML = "";
    }

    if (!emailPattern.test(email)) {
        emailErr.innerHTML = "Invalid email format";

    } else {
        emailErr.innerHTML = ""; 
    }
    if (!NamePattern.test(Name)) {
        nameErr.innerHTML = "Name only contains letters and must contain at least 4 letters";

    } else {
        nameErr.innerHTML = "";
    }
    if (!passwordPattern.test(passwrd)) {
        passwordErr.innerHTML = "Password must contain at least 6 characters";
    }else if (passwrd !== confirmPasswrd) {
        passwordErr.innerHTML = "Password doesn't match";
    } else {
        passwordErr.innerHTML = ""; 
    }

    if(!emailPattern.test(email) || !NamePattern.test(Name) || !passwordPattern.test(passwrd) || passwrd !== confirmPasswrd){
      return false;
    }
    return true; 
}
</script>

